{
  "dashboard": {
    "id": null,
    "title": "AgroTech Portugal - Sistema Overview",
    "tags": ["agrotech", "overview"],
    "timezone": "Europe/Lisbon",
    "panels": [
      {
        "id": 1,
        "title": "Usuários Ativos",
        "type": "stat",
        "targets": [
          {
            "expr": "flask_http_request_total",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            },
            "unit": "short"
          },
          "overrides": []
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "pluginVersion": "8.0.0",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Tempo de Resposta",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(flask_http_request_duration_seconds_sum[5m]) / rate(flask_http_request_duration_seconds_count[5m])",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "xAxis": {
          "buckets": null,
          "mode": "time",
          "name": null,
          "show": true,
          "values": []
        },
        "yAxes": [
          {
            "label": null,
            "max": null,
            "min": null,
            "show": true
          },
          {
            "label": null,
            "max": null,
            "min": null,
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Status dos Serviços",
        "type": "stat",
        "targets": [
          {
            "expr": "up",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN"
                  },
                  "1": {
                    "text": "UP"
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "green",
                  "value": 1
                }
              ]
            },
            "unit": "short"
          },
          "overrides": []
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        },
        "pluginVersion": "8.0.0",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Uso de Memória",
        "type": "graph",
        "targets": [
          {
            "expr": "node_memory_MemTotal_bytes - node_memory_MemAvailable_bytes",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Bytes",
            "max": null,
            "min": null,
            "show": true
          },
          {
            "label": null,
            "max": null,
            "min": null,
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Uso de CPU",
        "type": "graph",
        "targets": [
          {
            "expr": "100 - (avg by(instance) (rate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Percent",
            "max": 100,
            "min": 0,
            "show": true
          },
          {
            "label": null,
            "max": null,
            "min": null,
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 6,
        "title": "Requests por Minuto",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(flask_http_request_total[1m]) * 60",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/min",
            "max": null,
            "min": 0,
            "show": true
          },
          {
            "label": null,
            "max": null,
            "min": null,
            "show": true
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "version": 0
  }
}
